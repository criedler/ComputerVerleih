<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainPane" prefHeight="325.0" prefWidth="512.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="htl.steyr.computerRent.controller.ReturnController">
   <children>
      <ListView fx:id="customerView" layoutX="45.0" layoutY="29.0" onMouseClicked="#customerViewClicked" prefHeight="267.0" prefWidth="200.0" />
      <ButtonBar layoutX="271.0" layoutY="256.0" prefHeight="40.0" prefWidth="200.0">
         <buttons>
            <Button fx:id="returnContinue" mnemonicParsing="false" onAction="#rentalSelectContinue" text="Continue" />
            <Button mnemonicParsing="false" onAction="#rentalSelectBack" text="Go Back" />
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
